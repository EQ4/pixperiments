
MEMORY
{
    ram : ORIGIN = 0x8000, LENGTH = 0x60000
}

SECTIONS
{
    .text : { *(.text*) } > ram
/*    .bss : { *(.bss*) } > ram */
    .bss  : {
   __bss_start__ = .;
   *(.bss*)
   } > ram
   __bss_end__ = .;
   __bss_size__ = __bss_end__ - __bss_start__;
__exidx_start = .;
        .ARM.exidx : {*(.ARM.exidx* .gnu.linkonce.armexidx.*)} > ram
        __exidx_end = .;
}

